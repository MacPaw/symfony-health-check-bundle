{
    "name": "macpaw/symfony-health-check-bundle",
    "description": "Symfony health check bundle",
    "homepage": "https://github.com/MacPaw/symfony-health-check-bundle",
    "type": "symfony-bundle",
    "keywords": [
        "MacPaw",
        "Symfony",
        "monitoring",
        "healthcheck",
        "doctrine",
        "Symfony-Health-Check",
        "Symfony-Health-Check-Bundle",
        "Symfony-Health-Check-Doctrine"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yozhef Hisem",
            "email": "hisemjo@gmail.com",
            "homepage": "https://macpaw.com/",
            "role": "Software Engineer"
        },
        {
            "name": "Ivan Hisem",
            "email": "2yonchi@gmail.com",
            "homepage": "https://macpaw.com/",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^3.4 || ^4.1.12 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "ext-json": "*",
        "phpstan/phpstan": "0.12.*",
        "squizlabs/php_codesniffer": "3.5.*",
        "symfony/phpunit-bridge": "^3.4 || ^4.1.12 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symfony/yaml": "^3.4 || ^4.0 || ^5.0 || ^6.0",
        "symfony/browser-kit": "^3.4 || ^4.4 || ^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonyHealthCheckBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyHealthCheckBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": [
            "vendor/bin/phpcs"
        ],
        "cs-fix": [
            "vendor/bin/phpcbf"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse"
        ],
        "phpunit": [
            "vendor/bin/simple-phpunit"
        ]
    }
}
